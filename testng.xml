<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Tests for EAT Final Exam">
    <test name="Regression and Smoke Testing" thread-count="1" parallel="false">
        <groups>
            <run>
                <include name = "regression"></include>
            </run>
        </groups>
        <classes>
            <class name="DriverSetup.SetupDriver"/>
            <class name="Tests.AllTests"></class>
        </classes>
    </test>
</suite>
